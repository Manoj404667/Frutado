								<ParamGroup name="adv">
										<ParamInfo type="RELPOS" name="position_b" textkey="position_b"/>																							
										<ParamInfo type="SHELF" name="bubble_class" textkey="bubble_class" visibility="conditional">
                    		<Properties shelf="textstyles" specified="" />
                		</ParamInfo>
                		<ParamInfo type="SHELF" name="bubblestyle_d" textkey="bubblestyle_d" visibility="conditional">
                    		<Properties shelf="bubblestyles" specified="" />
                		</ParamInfo>										
										<ParamInfo type="INTEGER" name="b_width"></ParamInfo>
										<ParamInfo type="INTEGER" name="b_height"></ParamInfo>										
										<ParamInfo type="ORIENTATION_BUBBLE" name="orientation"></ParamInfo>										
										<ParamInfo type="POSITION" name="bubble_offset" NoExport="1"></ParamInfo>
										<ParamInfo type="BOOL_0_1" name="b_minimized"></ParamInfo>
